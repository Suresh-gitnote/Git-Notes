   "source": [
    "k=input(\"Enter a sentence:\")\n",
    "a = \" \"\n",
    "b = \" \"\n",
    "for i in k + \" \":\n",
    "    if i != \" \":\n",
    "        a = a + i\n",
    "    else:\n",
    "        if len(a) > len(b):\n",
    "            b = a\n",
    "        a = \"\"\n",
    "print(\"Long Word in the given sentence is >>>>\", b)"
